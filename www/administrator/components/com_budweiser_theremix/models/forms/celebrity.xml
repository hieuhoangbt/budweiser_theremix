<?xml version="1.0" encoding="utf-8"?>
<form>
  <fieldset>
    <field name="id" type="text" readonly="true" class="readonly" default="0" description="JGLOBAL_FIELD_ID_DESC"/>
    <field name="ordering"/>
    <field name="state" default="1" type="list" label="JSTATUS" description="JFIELD_PUBLISHED_DESC" class="inputbox" size="1">
      <option value="1">JPUBLISHED</option>
      <option value="0">JUNPUBLISHED</option>
      <option value="2">JARCHIVED</option>
      <option value="-2">JTRASHED</option>
    </field>
    <field name="checked_out" type="hidden" filter="unset"/>
    <field name="checked_out_time" default="0000-00-00 00:00:00" type="hidden" filter="unset"/>
    <field name="created_by" type="createdby" default="0" label="COM_BUDWEISER_THEREMIX_FORM_LBL_CELEBRITY_CREATED_BY" description="COM_BUDWEISER_THEREMIX_FORM_DESC_CELEBRITY_CREATED_BY" hidden="true" hint="COM_BUDWEISER_THEREMIX_FORM_LBL_CELEBRITY_CREATED_BY"/>
    <field name="modified_by" type="modifiedby" default="0" label="COM_BUDWEISER_THEREMIX_FORM_LBL_CELEBRITY_MODIFIED_BY" description="COM_BUDWEISER_THEREMIX_FORM_DESC_CELEBRITY_MODIFIED_BY" hidden="true" hint="COM_BUDWEISER_THEREMIX_FORM_LBL_CELEBRITY_MODIFIED_BY"/>
    <field name="name" required="true" label="COM_BUDWEISER_THEREMIX_FORM_LBL_CELEBRITY_NAME" description="COM_BUDWEISER_THEREMIX_FORM_DESC_CELEBRITY_NAME" hint="COM_BUDWEISER_THEREMIX_FORM_LBL_CELEBRITY_NAME"/>
    <field name="image_jpeg" type="media" required="true" label="Image jpeg" description="Image jpeg" hint="Image jpeg"/>
    <field name="image_png" type="media" required="true" label="Image png" description="Image png" hint="Image png"/>
    <field name="created_at" default="0000-00-00 00:00:00" label="COM_BUDWEISER_THEREMIX_FORM_LBL_CELEBRITY_CREATED_AT" description="COM_BUDWEISER_THEREMIX_FORM_DESC_CELEBRITY_CREATED_AT" hint="COM_BUDWEISER_THEREMIX_FORM_LBL_CELEBRITY_CREATED_AT"/>
    <field name="updated_at" default="0000-00-00 00:00:00" type="calendar" class="inputbox" filter="safehtml" format="%Y-%m-%d %H:%M:%S" label="COM_BUDWEISER_THEREMIX_FORM_LBL_CELEBRITY_UPDATED_AT" description="COM_BUDWEISER_THEREMIX_FORM_DESC_CELEBRITY_UPDATED_AT" hint="COM_BUDWEISER_THEREMIX_FORM_LBL_CELEBRITY_UPDATED_AT"/>
    <field name="version_note" type="text" label="JGLOBAL_FIELD_VERSION_NOTE_LABEL" description="JGLOBAL_FIELD_VERSION_NOTE_DESC" class="inputbox" size="45" labelclass="control-label"/>
  </fieldset>
</form>
